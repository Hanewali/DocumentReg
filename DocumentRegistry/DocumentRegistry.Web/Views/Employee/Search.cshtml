@model DocumentRegistry.Web.Models.Employee.Search

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>

<table class="table">
<tr>
    <td>@Html.DisplayNameFor(x => x.Employees.First().FirstName)</td>
    <td>@Html.DisplayNameFor(x => x.Employees.First().LastName)</td>
</tr>

@foreach (var employee in Model.Employees)
{
    <tr>
        <td>@employee.FirstName</td>
        <td>@employee.LastName</td>
    </tr>
}
</table>


</body>
</html>